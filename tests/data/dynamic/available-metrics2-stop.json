{
  "kind": "non_counter.generic",
  "version": 1,
  "parser_name": "available-metrics2-stop",
  "data_format": {
    "name":"simple_format"
  },
  "platforms": [
    "Platform1",
    "Platform2"
  ],
  "metrics_to_skip": [],
  "available_metrics": ["Metric1", "Met3"],
  "on_metric_check_failed": "stop",
  "titles_to_skip": [],
  "dimensions_to_skip": {},
  "metric_aliases": [
      ["Metric2", "M2"],
      ["Metric3", "Met3"]
  ],
  "dimension_aliases": [],
  "heuristics": {
    "conds": [
      {
        "pattern": "^Month$",
        "coord": {
          "row": 0,
          "col": 0
        },
        "kind": "regex"
      }
    ],
    "kind": "and"
  },
  "areas": [
    {
      "data_headers": {
        "roles": [
            {
                "source": {
                    "coord": {
                      "row": 0,
                      "col": 1
                    },
                    "direction": "right"
                },
                "role": "metric"
            }
        ],
        "data_direction": "down",
        "data_cells": {
          "coord": {
            "row": 1,
            "col": 1
          },
          "direction": "right"
        }
      },
      "dates": {
          "source": {
              "coord": {
                  "row": 1,
                  "col": 0
              },
              "direction": "down"
          },
          "extract_params": {
              "on_validation_error": "stop"
          },
          "role": "date"
      },
      "titles": null,
      "title_ids": [],
      "dimensions": [],
      "aggregate_same_records": true,
      "kind": "non_counter.generic"
    }
  ]
}
