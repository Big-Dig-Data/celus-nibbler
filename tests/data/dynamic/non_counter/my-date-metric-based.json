{
  "kind": "non_counter.date_metric_based",
  "version": 1,
  "parser_name": "my-date-metric-based",
  "data_format": {
    "name":"simple_format"
  },
  "platforms": [
    "My Platform"
  ],
  "metrics_to_skip": [],
  "titles_to_skip": [],
  "dimensions_to_skip": [],
  "heuristics": {
    "conds": [
      {
        "pattern": "^Extra$",
        "coord": {
          "row": 0,
          "col": 0
        },
        "kind": "regex"
      },
      {
        "max": 0,
        "kind": "sheet_idx"
      }
    ],
    "kind": "and"
  },
  "areas": [
    {
       "data_headers": {
           "roles": [
               {
                   "role": "metric",
                   "source": {
                       "coord": {
                           "col": 2,
                           "row": 0 
                        },
                        "direction": "right"
                   },
                   "regex": "^([^ ]+.+[^ ]+) in"
               },
               {
                   "role": "date",
                   "source": {
                       "coord": {
                           "col": 2,
                           "row": 0
                        },
                        "direction": "right"
                   },
                   "regex": "in (\\d+\\/\\d+)$"
               }
           ],
           "data_cells": {
               "coord": {
                   "row": 1,
                   "col": 2
               },
               "direction": "right"
           },
           "data_direction": "down"
      },
      "organizations": {
          "source": {
              "coord": {
                  "row": 1,
                  "col": 1
              },
              "direction": "down"
          },
          "regex": "^MYCONS - (.*)$"
      },
      "titles": null,
      "title_ids": [],
      "dimensions": [
        {
          "name": "Extra",
          "source": {
            "coord": {
              "row": 1,
              "col": 0
            },
            "direction": "down"
          },
          "required": true
        }
      ],
      "kind": "non_counter.date_metric_based"
    }
  ]
}
