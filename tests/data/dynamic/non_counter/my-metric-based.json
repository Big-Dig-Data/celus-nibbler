{
  "kind": "non_counter.generic",
  "version": 1,
  "parser_name": "my-metric-based",
  "data_format": {
    "name":"simple_format"
  },
  "platforms": [
    "My Platform"
  ],
  "metrics_to_skip": [],
  "titles_to_skip": [],
  "dimensions_to_skip": {},
  "heuristics": {
    "conds": [
      {
        "pattern": "^My Online Summary Usage Report$",
        "coord": {
          "row": 0,
          "col": 0
        },
        "kind": "regex"
      },
      {
        "cond": {
            "pattern": "^ips_",
            "kind": "sheet_name"
        },
        "kind": "neg"
      }
    ],
    "kind": "and"
  },
  "areas": [
    {
       "data_headers": {
           "roles": [
               {
                   "role": "metric",
                   "source": {
                       "coord": {
                           "col": 7,
                           "row": 14
                        },
                        "direction": "right"
                   }
               }
           ],
           "data_cells": {
               "coord": {
                   "row": 15,
                   "col": 7
               },
               "direction": "right"
           },
           "data_direction": "down"
      },
      "organizations": {
          "source": {
              "coord": {
                  "row": 15,
                  "col": 3
              },
              "direction": "down"
          },
          "extract_params": {
            "on_validation_error": "stop"
          }
      },
      "dates": {
          "source": {
            "coord": {
              "row": 15,
              "col": 1
            },
            "direction": "down"
          }
      },
      "titles": null,
      "title_ids": [],
      "dimensions": [
        {
          "name": "Dimension1",
          "source": {
            "coord": {
              "row": 15,
              "col": 2
            },
            "direction": "down"
          }
        }
      ],
      "aggregate_same_records": true,
      "kind": "non_counter.generic"
    }
  ]
}
