{
  "kind": "non_counter.generic",
  "version": 1,
  "parser_name": "date-in-single-field",
  "data_format": {
    "name": "simple_format"
  },
  "platforms": [
    "Platform1",
    "Platform2"
  ],
  "metrics_to_skip": [],
  "titles_to_skip": [],
  "dimensions_to_skip": {},
  "metric_aliases": [],
  "dimension_aliases": [],
  "heuristics": {
    "conds": [
      {
        "pattern": "^Title$",
        "coord": {
          "row": 2,
          "col": 0
        },
        "kind": "regex"
      }
    ],
    "kind": "and"
  },
  "areas": [
    {
      "data_headers": {
        "roles": [
            {
                "source": {
                    "coord": {
                      "row": 2,
                      "col": 1
                    },
                    "direction": "right"
                },
                "role": "metric"
            }
        ],
        "data_direction": "down",
        "data_cells": {
          "coord": {
            "row": 3,
            "col": 1
          },
          "direction": "right"
        }
      },
      "dates": {
          "source": {
              "row": 0,
              "col": 0
          },
          "role": "date"
      },
      "aggregate_same_records": true,
      "kind": "non_counter.generic"
    }
  ]
}
